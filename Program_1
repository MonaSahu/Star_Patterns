using System;
					
public class Program1
{
	public static void Main()
	{
		Console.WriteLine("Enter the number:");
		int n = Convert.ToInt32(Console.ReadLine());
		for(int i = 0 ; i < n ; i++)
		{
			for(int j = 0; j < n ; j++)
			{
				Console.Write("*");
				
			}
			Console.WriteLine();
		}
	}
}
